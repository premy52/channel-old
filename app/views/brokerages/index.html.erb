<h1>Listing brokerages</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @brokerages.each do |brokerage| %>
      <tr>
        <td><%= brokerage.company %></td>
        <td><%= brokerage.address1 %></td>
        <td><%= brokerage.address2 %></td>
        <td><%= brokerage.city %></td>
        <td><%= brokerage.state %></td>
        <td><%= brokerage.zip %></td>
        <td><%= brokerage.rate %></td>
        <td><%= link_to 'Show', brokerage %></td>
        <td><%= link_to 'Edit', edit_brokerage_path(brokerage) %></td>
        <td><%= link_to 'Destroy', brokerage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Brokerage', new_brokerage_path %>
